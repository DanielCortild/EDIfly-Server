<h1>Faqs</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Filename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faqs.each do |faq| %>
      <tr>
        <td><%= faq.title %></td>
        <td><%= faq.filename %></td>
        <td><%= link_to 'Show', faq, :class => "btn btn-primary" %></td>
        <td><%= link_to 'Edit', edit_faq_path(faq), :class => "btn btn-warning" %></td>
        <td><%= link_to 'Destroy', faq, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Faq', new_faq_path, :class => "btn btn-success" %>
